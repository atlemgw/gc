from tkinter import *

# Tk()
window = Tk()

label1 = Label(window, text="Label 1")

label2 = Label(window, text="Label 2")

label3 = Label(window, text = "Label 3")

label1.grid(row = 0, column = 0)

label2.grid(row = 0, column = 1)

label3.grid(row = 1, column = 0)

# Mainloop af vindue
window.mainloop()
