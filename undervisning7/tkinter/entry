from tkinter import *

window = Tk()

label1 = Label(window, text="Navn:")

label1.grid(row = 0, column = 0, sticky ="E")

label2 = Label(window, text = "Password")

label2.grid(row=1, column=0, sticky = "E")

entrySpace = Entry(window)

entrySpace.grid(row = 0, column = 1)

entrySpace2 = Entry(window)

entrySpace2.grid(row = 1, column = 1)

cbutton = Checkbutton(window, text = "Husk Password")

cbutton.grid(columnspan = 2)


window.mainloop()
